<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/ticket_history_element"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="50dp"
        android:background="@drawable/report_shape">
        <LinearLayout
            android:id="@+id/ticket_history_element_small"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="center">
            <TextView
                android:id="@+id/ticket_history_element_date"
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:textColor="@color/black"
                android:text="28.01.2022"
                android:textStyle="bold"
                android:textAlignment="center">
            </TextView>

            <TextView
                android:id="@+id/ticket_history_element_line_number"
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:text="Nr lini 105"
                android:textColor="@color/black"
                android:textStyle="normal"
                android:textAlignment="center">

            </TextView>

        </LinearLayout>
</LinearLayout>